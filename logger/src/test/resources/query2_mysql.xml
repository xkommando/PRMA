    <entry key="MarkerDAO.putMarker"><![CDATA[
        INSERT INTO `marker_name`(`id`, `value`)VALUES(?,?)
    ]]></entry>

    <entry key="MarkerDAO.getMarkerIDs"><![CDATA[
        SELECT `id` FROM `marker_name` LIMIT ?
    ]]></entry>


    <entry key="MarkerDAO.putRelation"><![CDATA[
    INSERT INTO `j_event_marker`(`marker_id`, `event_id`)VALUES(?,?)
    ]]></entry>


    <!--<entry key="PropertyDAO.getByEvent"><![CDATA[-->
        <!--SELECT PROP.`key`, PROP.`value` FROM `property` as PROP-->
        <!--INNER JOIN j_event_prop as JEP on JEP.prop_id = PROP.id-->
        <!--WHERE JEP.event_id=?-->
    <!--]]></entry>-->

    <entry key="PropertyDAO.putMap"><![CDATA[
        INSERT IGNORE INTO `property`(`id`, `key`, `value`)VALUES (?,?,?)
    ]]></entry>

    <entry key="PropertyDAO.putRelation"><![CDATA[
        INSERT INTO `j_event_prop`(`prop_id`, `event_id`)VALUES (?,?)
    ]]></entry>

    <entry key="PropertyDAO.getPropertyIDs"><![CDATA[
        SELECT `id` FROM `property` LIMIT ?
    ]]></entry>

