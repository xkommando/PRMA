        <!--WHERE JEE.event_id=? ORDER BY JEE.seq ASC-->
    <!--]]></entry>-->

    <entry key="ExceptionDAO.putExcept"><![CDATA[
        INSERT INTO `exception`(`id`,`name`,`msg`)
        VALUES (?,?,?)
    ]]></entry>

    <entry key="ExceptionDAO.putStackTraceExceptRelation"><![CDATA[
        INSERT INTO `j_exception_stacktrace`(`seq`,`stacktrace_id`,`except_id`)VALUES(?,?,?)
    ]]></entry>

    <entry key="ExceptionDAO.putEventExceptRelation"><![CDATA[
        INSERT INTO `j_event_exception`(`seq`,`event_id`,`except_id`)VALUES(?,?,?)
    ]]></entry>

    <entry key="ExceptionDAO.getExceptIDs"><![CDATA[
        SELECT `id` FROM `exception` LIMIT ?
    ]]></entry>

</properties>